{
    "env": {
        "browser": true,
        "es2021": true 
    },
    "extends": [ 
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "react/prop-types": 0
    }
}